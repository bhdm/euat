            $result1 = (function() {

                var gender = $0;
                var race = $1;
                var age = $2;
                var creatinine = $3;

                var MDRDconstant;

                if(gender == 1.0) { //male
                    MDRDconstant = 1.0;
                }
                else {
                    MDRDconstant = 0.742;
                }

                if(race == 1.0) { //white
                    MDRDconstant *= 1.0;
                }
                else {
                    MDRDconstant *= 1.21;
                }

                var mdrdResults = (186*MDRDconstant)/(Math.pow(creatinine,1.154) * Math.pow(age,0.203));

                return Math.round(mdrdResults * 10.0)/10.0;

            })();
            append_to_debug('$result' + 1 + " is " + $result1);

