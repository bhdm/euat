{% extends 'AppBundle::layout.html.twig' %}

{% block content %}
    <h1 class="blue-text">Тест</h1>
    <div>
        <div class="row">
            <div class="row">
                <div class=" col-xs-offset-4 col-xs-4">
                    <div class="dump"></div>
                    <div class="question_0_error"></div>
                    <div class="question_1_error"></div>
                    <div class="question_2_error"></div>
                    <div class="question_3_error"></div>
                    <div class="question_4_error"></div>
                </div>
            </div>
            <div class=" col-xs-offset-4 col-xs-2">
                Пол
            </div>
            <div class="col-xs-4">
                <select class="question_0 form-control" onchange="compute();">
                    <option value="1">Мужской</option>
                    <option value="0">Женский</option>
                </select>
            </div>
        </div>
        <br />
        <div class="row">
            <div class=" col-xs-offset-4 col-xs-2">
                Рассовая принадлежность
            </div>
            <div class="col-xs-4">
                <select class="question_1 form-control" onchange="compute();">
                    <option value="1">Афроамериканец</option>
                    <option value="1.156">Другой</option>
                </select>
            </div>
        </div>
        <br />
        <div class="row">
            <div class=" col-xs-offset-4 col-xs-2">
                Возраст (Лет)
            </div>
            <div class="col-xs-4">
                <input type="text" onkeyup="compute();" class="question_2 form-control" name="question_2" placeholder="Возраст">
                <input type="hidden" class="question_2_units" name="question_2_units" value="1">
            </div>
        </div>
        <br />
        <div class="row">
            <div class=" col-xs-offset-4 col-xs-2">
                Креатинин
            </div>
            <div class="col-xs-2">
                <input type="text" onkeyup="compute();" class="question_3 form-control" name="question_3" placeholder="Креатинин">
            </div>
            <div class="col-xs-2">
                <select class="form-control question_3_units" onchange="compute();">
                    <option value="1.0">mg/dL</option>
                    <option value="0.011312217">µmol/L</option>
                </select>
            </div>
        </div>
        <div class="row">
            <div class=" col-xs-offset-4 col-xs-4">
                <div class="result_1_title_area"></div>
                <div class="result_1_sub_title_area"></div>
                <div class="result_1_answer_area"></div>
            </div>

        </div>
    </div>
{% endblock %}

